WSGIPythonPath {{venv}}/lib/python3.3/site-packages
WSGIDaemonProcess frs user={{user}} group=apache processes=1 threads=1 python-path={{venv}}/lib/python3.3/site-packages
WSGIProcessGroup frs

<Directory />
    Options FollowSymLinks
    AllowOverride None
    Order deny,allow
    Allow from all
</Directory>

<VirtualHost *:80>
	WSGIApplicationGroup %{GLOBAL}
	WSGIPassAuthorization On
	WSGIScriptAlias / {{repo}}/edware/hpz/frs.wsgi
	WSGIImportScript {{repo}}/edware/hpz/frs.wsgi process-group=frs application-group=%{GLOBAL}
	<Directory {{repo}}/edware/hpz/frs>
  		Order allow,deny
  		Allow from all
	</Directory>
</VirtualHost>

Listen 80
LoadModule wsgi_module libexec/apache2/mod_wsgi.so
